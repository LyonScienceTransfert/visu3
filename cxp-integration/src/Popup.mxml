<?xml version="1.0" encoding="utf-8"?>
<s:Application xmlns:fx="http://ns.adobe.com/mxml/2009" 
			   xmlns:s="library://ns.adobe.com/flex/spark" 
			   xmlns:mx="library://ns.adobe.com/flex/mx" minWidth="1024" minHeight="768" skinClass="skins.PageSkin">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	<s:Group top="68" width="1024" minHeight="667" horizontalCenter="0">
		
		<!-- container -->
		<s:Group width="1004" height="100%" horizontalCenter="0">
			
			<!-- background -->
			<s:Group width="100%" height="100%">
				<s:Rect width="100%" height="100%" radiusX="5">
					<s:fill>
						<s:SolidColor color="0xffefd1" />
					</s:fill>
				</s:Rect>
				
				<s:Rect width="100%" height="100%" radiusX="5">
					<s:stroke>
						<s:SolidColorStroke color="0x9f9f9f" />
					</s:stroke>
				</s:Rect>
			</s:Group>
			
			<!-- Panneaux -->
			<s:Group width="100%" height="100%">
				
				<s:Panel left="1" top="1" bottom="1" width="290" height="100%" skinClass="skins.FlexiblePanel" title="Explorer">
					
				</s:Panel>
				
				<s:Panel right="1" top="1" bottom="1" width="694" height="100%" skinClass="skins.FlexiblePanel" title="Détail de la séance">
					
				</s:Panel>
				
			</s:Group>
			<!-- Fin Group Panneaux -->
		</s:Group>
		<!-- Fin Group Container -->
	</s:Group>
	
	<s:Group width="100%" height="100%">
		<s:Rect width="100%" height="100%" >
			<s:fill>
				<s:SolidColor color="0x000000" alpha="0.42" />
			</s:fill>
		</s:Rect>
		<s:Group width="300" height="200" horizontalCenter="0" verticalCenter="0">
			<s:Rect width="100%" height="100%" left="0" top="0" right="0" bottom="0" radiusX="5">
				<s:filters>
					<s:DropShadowFilter 
						blurX="40" blurY="40" 
						alpha="0.7" 
						distance="1" 
						angle="90"                 
						knockout="true"/>
				</s:filters>
				<s:fill>
					<s:SolidColor color="0x000000"/>
				</s:fill>
			</s:Rect>
			<s:Rect width="100%" height="100%" radiusX="5">
				<s:fill>
					<s:LinearGradient rotation="90">
						<s:GradientEntry color="0xFFEECF" />
						<s:GradientEntry color="0xFFFFFF" />
					</s:LinearGradient>
				</s:fill>
				<s:stroke>
					<s:SolidColorStroke color="0xffb311" />
				</s:stroke>
			</s:Rect>
			<s:Group width="100%" height="100%" left="3" top="3" right="3" bottom="3">
				
				<!-- Header -->
				<s:Group width="100%" height="26">
					<s:Rect width="100%" height="100%" radiusX="5">
						<s:fill>
							<s:LinearGradient rotation="90">
								<s:GradientEntry color="0xfabf30" />
								<s:GradientEntry color="0xffae00" />
							</s:LinearGradient>
						</s:fill>
					</s:Rect>
					<s:Rect width="100%" height="5" bottom="0">
						<s:fill>
							<s:SolidColor color="0xffae00" />
						</s:fill>
					</s:Rect>
					<s:Label left="5" height="26" fontSize="14" fontWeight="bold"
							 text="Titre" verticalAlign="middle"/>
					<s:Button right="3" top="4" skinClass="skins.Seance.CloseButton"/>
				</s:Group>
				
				<!-- Pannels -->
				<s:Group top="33" width="100%" height="100%">
					<s:HGroup width="100%" height="100%" gap="10">
						
						<!-- Pannel "display filter" -->
						<!-- Pannel "User list" -->
						
						<!-- Pannel "User info" -->
					</s:HGroup>
					<!-- End of Pannels -->
				</s:Group>
				
				<!-- Buttons -->
				<s:HGroup height="23" bottom="0" right="2">
					<s:Button skinClass="skins.Components.Button" label="OK" />
					<s:Button skinClass="skins.Components.Button" label="Annuler" />
				</s:HGroup>
				
			</s:Group>
			
		</s:Group>
	</s:Group>
	
</s:Application>
