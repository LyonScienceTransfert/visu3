<!--- The default skin class for the Spark Button component.  

@see spark.components.Button

@langversion 3.0
@playerversion Flash 10
@playerversion AIR 1.5
@productversion Flex 4
-->
<s:SparkButtonSkin xmlns:fx="http://ns.adobe.com/mxml/2009"
				   xmlns:s="library://ns.adobe.com/flex/spark"
				   xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
				   minWidth="21" minHeight="21"
				   alpha.disabled="0.3"
				   width="87" height="48">
	
	<fx:Metadata>
		<![CDATA[ 
		/** 
		* @copy spark.skins.spark.ApplicationSkin#hostComponent
		*/
		[HostComponent("spark.components.Button")]
		]]>
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="active" />
		<s:State name="disabled" />
	</s:states>
	
	
	
	<!-- layer 8: text -->
	<!--- @copy spark.components.supportClasses.ButtonBase#labelDisplay  -->
	<s:Image x="0" y="0" scaleMode="stretch" source="images/Menu/ProfilUp.png"
			 source.active="@Embed('images/Menu/ProfilDown.png')"
			 source.disabled="@Embed('images/Menu/ProfilDisabled.png')"
			 source.down="@Embed('images/Menu/ProfilDown.png')"
			 source.over="@Embed('images/Menu/ProfilOver.png')"
			 source.up="@Embed('images/Menu/ProfilUp.png')"/>
	<s:Label id="labelDisplay" 
			 maxDisplayedLines="1" textAlign="center" verticalAlign="middle"
			 left="11" right="9" top="32" bottom="2" 
			 color="#FFFFFF" color.active="#010101" color.down="#010101"
			 fontSize="11" horizontalCenter="1" verticalCenter="15"
			 fontWeight.active="bold">
	</s:Label>
	
</s:SparkButtonSkin>
