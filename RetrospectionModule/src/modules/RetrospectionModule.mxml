						var sessionSalonRetroEvent:SessionEvent = new SessionEvent(SessionEvent.GET_SESSION_SALON_RETRO);
						sessionSalonRetroEvent.sessionId = sessionId;
						this.dispatchEvent(sessionSalonRetroEvent);
					}
