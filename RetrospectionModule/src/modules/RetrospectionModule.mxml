			/**
			 * play/pause of the streams
			 */
			private function onCheckSeekStream(event:MessageEvent):void
			{
				if(Model.getInstance().getCurrentRetroModule() == this)
				{
					var timeStartSession:Number = this.testTimeLine.startTimeSession;
					var playTime:Number = 0;
					// set currentRetrocumentSegmentPlaying to null
					setCurrentRetroDocumentSegmentPlaying(null);
					// remove timer RetroDocumentSegment
					removeTimerRetroDocumentSegment();
					// decale the time start for time on paused
					var timePausedNumber:Number = new Date().time -  this.timePause;
					var typeMessage:String =""
					typeMessage = event.message;
					if (typeMessage == "play")
					{
						this.visio.resumeStreams();
						// set status play
						this.visio.status = VisuVisio.STATUS_REPLAY;
						this.timeStart = this.timeStart  + timePausedNumber;
						this.timer.start();
						this.timePause = 0;
						// add obsel User Action
						var actionVideoPlayer:SalonRetroEvent = new SalonRetroEvent(SalonRetroEvent.ACTION_ON_VIDEO_PLAYER);
						actionVideoPlayer.typeAction = TraceModel.RETRO_PLAY_VIDEO_EVENT;
						playTime = timeStartSession + timePausedNumber;
						actionVideoPlayer.timePlayer = playTime;
						this.dispatchEvent(actionVideoPlayer);
					}else
					{
						pauseVideo();
					}
				}
			}
			
			private function pauseVideo():void
			{
				if(Model.getInstance().getCurrentRetroModule() == this)
				{
					var timeStartSession:Number = this.testTimeLine.startTimeSession;
					var playTime:Number = 0;
					this.visio.pauseStreams();
					// set status pause
					this.visio.status = VisuVisio.STATUS_PAUSE;
					this.timer.stop();
					// set time pause
					this.timePause = new Date().time;
					// add obsel User Action
					var actionVideoPlayer:SalonRetroEvent = new SalonRetroEvent(SalonRetroEvent.ACTION_ON_VIDEO_PLAYER);
					actionVideoPlayer.typeAction = TraceModel.RETRO_PAUSE_VIDEO_EVENT;
					playTime = new Date().time - this.timeStart + this.deltaTime + timeStartSession;
					actionVideoPlayer.timePlayer = playTime;
					this.dispatchEvent(actionVideoPlayer);
					// update buttons play on the retroDocumentSegments
					updateLabelButtonPlayStopRetroDocumentSegment();
				}
			}
			private function prePlayVideoFlux(value:Number,delta:Number = 0):void
			{
				if(delta != 0)
				{
					value = delta + this.testTimeLine.startTimeSession;
				}else
				{
					delta = value;
					value = value + this.testTimeLine.startTimeSession;
				}
				trace("prePlayVideoFlux value= "+value.toString());
				trace("prePlayVideoFlux delta= "+delta.toString());
				
				updateTimeCursor(delta);
				
				
				this.pickCheryClick = value;
				
				// remove all streams and listeners
				this.removeAllStreams();
				var listObselSessionIn:ArrayCollection = Model.getInstance().getObselSessionInByTimestamp(value);
				// update checking sum flux video
				this.checkingSumFluxVideo = this.getSumBeginTimeObselSI(listObselSessionIn);
				this.addFluxVideo(listObselSessionIn);
				// running the flux video
				if(!this.timer.running)
				{
					var timePausedNumber:Number = new Date().time -  this.timePause;
					this.timeStart = this.timeStart  + timePausedNumber;
					this.timer.start();
					this.sessionTimeLine.setShowButtonNavigation(true);
				}
				// update buttons play/stop
				this.sessionTimeLine.setStatusPlay(true);
				
				this.timePause = 0; 
				
				// add obsel User Action
				var actionVideoPlayer:SalonRetroEvent = new SalonRetroEvent(SalonRetroEvent.ACTION_ON_VIDEO_PLAYER);
				actionVideoPlayer.typeAction = TraceModel.RETRO_VIEDO_GO_TO_TIME_EVENT;
				// choosing time by user
				actionVideoPlayer.timePlayer = this.pickCheryClick;
				this.dispatchEvent(actionVideoPlayer);
			}

		]]>
		
