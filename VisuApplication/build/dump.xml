      <show-shadowed-device-font-warnings>true</show-shadowed-device-font-warnings>
      <!-- compiler.show-unused-type-selector-warnings: basculer vers l'affichage des avertissements générés par les sélecteurs de type CSS inutilisés-->
      <show-unused-type-selector-warnings>true</show-unused-type-selector-warnings>
