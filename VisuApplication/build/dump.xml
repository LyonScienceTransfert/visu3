      <!-- compiler.warn-assignment-within-conditional: Attribution au sein d'un élément conditionnel.-->
      <warn-assignment-within-conditional>true</warn-assignment-within-conditional>
      <!-- compiler.warn-bad-array-cast: Opération de projection de réseau potentiellement incorrecte.-->
      <warn-bad-array-cast>true</warn-bad-array-cast>
      <!-- compiler.warn-bad-bool-assignment: Une valeur non booléenne a été utilisée alors qu'une valeur booléenne était attendue.-->
      <warn-bad-bool-assignment>true</warn-bad-bool-assignment>
      <!-- compiler.warn-bad-date-cast: Opération de projection de données incorrecte.-->
      <warn-bad-date-cast>true</warn-bad-date-cast>
