      <show-actionscript-warnings>true</show-actionscript-warnings>
      <!-- compiler.show-binding-warnings: basculer vers l'affichage des avertissements générés par le code de liaison des données-->
      <show-binding-warnings>true</show-binding-warnings>
      <!-- compiler.show-invalid-css-property-warnings: basculer vers le rapport d'avertissements de propriété css non valides-->
